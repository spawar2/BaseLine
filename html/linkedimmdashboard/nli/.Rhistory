library(ggplot2)
haiStudy <- read.csv("/Applications/AMPPS/www/linkedimmDashBoard/nli/demosaved.csv")
#studyaccession<-args[1]
p <- ggplot(haiStudy,
aes(x=s.accession, y=p.age)) +
geom_boxplot(outlier.colour = NA) +
theme(axis.text.x = element_text(angle = 90)) +
geom_point()
ggsave(plot = p,
filename = "nli_plot.png",
path = "/Applications/AMPPS/www/linkedimmDashBoard/nli",
device = "png")
